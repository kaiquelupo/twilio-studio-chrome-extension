{
  "name": "Studio Extension",
  "version": "1.0",
  "description": "This extension adds features to Studio",
  "manifest_version": 2,
  "permissions": [
    "tabs","storage"
  ],
  "content_scripts": [
      {
        "matches": [
            "https://www.twilio.com/console/studio/flows/*/revisions"
        ],
        "js": ["index.js"]
      },
      {
        "matches": [
            "https://www.twilio.com/console/project/users"
        ],
        "js": ["users.js"]
      }
  ]
}
